{
  "name": "minute-tick-processor",
  "version": "1.0.0",
  "main": "src/index",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "pretest": "npm run lint",
    "test": "ava -T 30s test/*.test.js",
    "simulate": "gcloud pubsub topics publish minute-tick --message minute-tick",
    "deploy": "gcloud functions deploy subscribe --trigger-topic minute-tick --runtime=nodejs10 --region=europe-west1"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^0.28.1",
    "safe-buffer": "^5.1.2"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.2.0",
    "ava": "^1.4.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.2"
  }
}
