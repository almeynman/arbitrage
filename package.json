{
  "name": "arbitrage",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "bootstrap": "npx lerna bootstrap --hoist && npm i && lerna exec npm i && npm run build",
    "build": "lerna exec npm run build",
    "clean": "lerna clean && rm -rf ./node_modules/ && find . -name \"package-lock.json\" -delete",
    "lint": "eslint --format=codeframe '**/*.js'",
    "test": "lerna run test",
    "update-version": "lerna version",
    "deploy": "lerna run deploy"
  },
  "devDependencies": {
    "lerna": "^3.13.3",
    "prettier": "^1.18.2",
    "tar": ">=2.2.2",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-origin-ordered-imports-rule": "^1.2.2",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=codeframe"
    ]
  },
  "dependencies": {}
}
