    "serverless-offline": "^5.6.1",
    "serverless-offline-scheduler": "^0.3.7",
    "serverless-offline-sns": "^0.53.0",
    "serverless-plugin-optimize": "^4.1.0-rc.1",
    "serverless-plugin-typescript": "^1.1.7",
    "sinon": "7.3.2",
    "sinon-chai": "^3.3.0",
    "ts-jest": "24.0.2",
    "typescript": "^3.5.2"
  },
