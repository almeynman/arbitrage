{
  "name": "arbitrage-checker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "start": "npm run tsc && node dist/server.js",
    "create-queues": "npm run create-send-exchange-pairs && npm run create-dispatch-with-common-symbols && npm run create-assess",
    "create-send-exchange-pairs": "aws --endpoint http://localhost:4576 sqs create-queue --queue-name send-exchange-pairs --region us-east-1",
    "create-dispatch-with-common-symbols": "aws --endpoint http://localhost:4576 sqs create-queue --queue-name dispatch-with-common-symbols --region us-east-1",
    "create-assess": "aws --endpoint http://localhost:4576 sqs create-queue --queue-name assess --region us-east-1",
    "trigger-send-exchange-pairs": "aws --endpoint-url=http://localhost:4576 sqs send-message --queue-url http://localhost:4576/queue/send-exchange-pairs --region us-east-1 --message-body 'Wassup?'"
  },
  "dependencies": {
    "@google-cloud/firestore": "^2.2.3",
    "aws-sdk": "^2.501.0",
    "ccxt": "^1.18.900",
    "core": "../packages/core",
    "implementation": "../packages/implementation",
    "js-yaml": "3.13.1",
    "sqs-consumer": "^5.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.28",
    "@types/chai": "^4.1.7",
    "@types/jest": "^24.0.13",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "@types/uuid": "^3.4.5",
    "chai": "^4.2.0",
