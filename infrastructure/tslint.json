{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-unused-imports": true,
        "no-unused-variable": true,
        "semicolon":[true, "never"],
        "quotemark": [true, "single"],
        "trailing-comma":[false],
        "interface-name":[true, "never-prefix"]
    },
    "rulesDirectory": []
}
