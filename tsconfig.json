{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es5", "es6", "dom"],
    "jsx": "react",
  },
  "include": [
    "./core/**/*",
    "./implementation/**/*",
    "./infrastructure/**/*",
    "./application/**/*",
  ]
}
