{
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "pretest": "npm run lint",
    "test": "ava -T 30s *.test.js",
    "preinstall": "npx package-json-merge ../shared-package.json ./package.json > ./package-tmp.json && rm ./package.json && mv ./package-tmp.json ./package.json",
    "prepublishOnly": "npm --no-git-tag-version version patch",
    "deploy": "npm publish"
  },
  "dependencies": {
    "arbitrage-lib": "^1.0.0",
    "safe-buffer": "^5.1.2"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1"
  },
  "name": "arbitrage-lib",
  "version": "1.0.18"
}
