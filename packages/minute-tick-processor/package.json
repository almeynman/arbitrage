    "arbitrage-lib": "github:almeynman/arbitrage-lib#v1.0.18"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.2.0",
    "ava": "^1.4.1",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.2"
  }
}
